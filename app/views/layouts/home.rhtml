<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<title>Bulldog Tennis League</title>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
<style type="text/css">
<!--
body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
}
-->
</style>
<link href="/stylesheets/style.css" rel="stylesheet" type="text/css">
<script src="/javascripts/tooltip.js" type="text/javascript"></script>
<link href="/stylesheets/tooltip.css" rel="stylesheet" type="text/css">
<style type="text/css">
<!--
.style2 {font-size: 18px}
-->
</style>
<style type="text/css">
.slideshow { height: 120px; width: 170px; margin: auto }

</style>
<!-- include jQuery library -->
<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.4.1/jquery.min.js"></script>

<!-- include Cycle plugin -->
<script type="text/javascript" src="http://cloud.github.com/downloads/malsup/cycle/jquery.cycle.all.2.74.js"></script>

<!--  initialize the slideshow when the DOM is ready -->
<script type="text/javascript">
$(document).ready(function() {
    $('.slideshow').cycle({
		fx: 'turnDown' // choose your transition type, ex: fade, scrollUp, shuffle, etc...
	});
});
</script>
</head>

<body>
<table width="1000px"  border="0" align="center" cellpadding="0" cellspacing="0">
  <tr >
    <td colspan="3" background="/images/top_bg.jpg"><table width="950" border="0" align="center" cellpadding="0" cellspacing="0">
      <tr>
        <td width="116"><img src="/images/logo.png" width="168" height="194"></td>
        <td width="834"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
          <tr>
            <td width="479" align="right"><img src="/images/wherewinning_text.png" width="300" height="87"></td>
            <td width="25">&nbsp;</td>
            <td width="278" height="182" background="/images/login_bg.png" style="background-repeat:no-repeat;">
              <% form_tag session_path do -%>
              <table width="100%"  border="0" cellspacing="0" cellpadding="0">
                <tr>
                  <td width="4%" height="34">&nbsp;</td>
                  <td width="92%" class="white_text2">Members Log In </td>
                  <td width="4%">&nbsp;</td>
                </tr>
                <tr>
                  <td height="1"></td>
                  <td height="1" bgcolor="#ffffff"></td>
                  <td height="1"></td>
                </tr>
                <tr>
                  <td>&nbsp;</td>
                  <td><table width="100%"  border="0" cellspacing="1" cellpadding="0">
                      <tr>
                        <td height="10" colspan="2"></td>
                        </tr>
                      <tr>
                        <td width="35%" align="right" class="black_text">UserId:</td>
                        <td width="65%">
                            <%= text_field_tag 'login', @login, :size => '22' %>
                        </td>
                      </tr>
                      <tr>
                        <td align="right" class="black_text">Password:</td>
                        <td>
                            <%= password_field_tag 'password', nil, :size => '22'  %>
                        </td>
                      </tr>
                      <tr>
                        <td>&nbsp;</td>
                        <td>
                            <input type="checkbox" name="checkbox" value="checkbox">
                            <span class="text">Remember me</span>
                        </td>
                      </tr>
                      <tr>
                        <td>&nbsp;</td>
                        <td><a href="#" class="content_red_text">Forgot your password?</a></td>
                      </tr>
                      <tr align="center">
                        <td height="30" colspan="2">
                           <%= submit_tag 'Log in', :class => 'login_button' %>
                        </td>
                      </tr>
                  </table>
                   <%end%>
                  </td>
                  <td>&nbsp;</td>
                </tr>
              </table>              </td>
          </tr>
        </table></td>
      </tr>
      <tr>
      <td> &nbsp;</td>
      <td height="35" align="right"><a href="#" class="main_link_highlight">Home</a> <span class="main_link">|</span>  <a href="/about_us" class="main_link">About Us</a> <span class="main_link">|</span> <a href="/contact_us" class="main_link">Contact Us</a></td>

      </tr>
    </table></td>
  </tr>
  <tr>
    <td width="26">&nbsp;</td>

    <td width="948"><%= yield %></td>
    <td width="26">&nbsp;</td>
  </tr>
  <tr>
    <td bgcolor="#666666">&nbsp;</td>
    <td height="41" bgcolor="#666666"><table width="100%"  border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td height="10" colspan="2"></td>
      </tr>
      <tr>
       <td width="64%" align="left"> <a href="/home" class="footer_link">Home</a> <span class="footer_link">|</span>  <a href="/about_us" class="footer_link">About Us</a> <span class="footer_link">|</span> <a href="/contact_us" class="footer_link">Contact Us</a></td>
        <td width="32%" align="center" class="copyright_text">Copyright &copy; 2009, Bulldog Tennis League. </td>
        </tr>
      <tr>
        <td height="10" colspan="2"></td>
      </tr>
    </table></td>
    <td bgcolor="#666666">&nbsp;</td>
  </tr>
</table>
</body>
</html>
